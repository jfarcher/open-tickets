body{
/*font-family:"Lucida Grande", "Lucida Sans Unicode", Verdana, Arial, Helvetica, sans-serif;*/
tr{
font-zise:11px;
border:solid 2px #b7ddf2;
margin-left:5px;
}
font-family:Verdana, Arial, Helvetica, sans-serif;
font-size:12px;
}
p, h1, form, button{border:0; margin:0; padding:0;}
.spacer{clear:both; height:1px;}
#nav {
	width: 100%;
	float: left;
	margin: 0 0 3em 0;
	padding: 0;
	list-style: none; 
	background-color: #f2f2f2;
	border-bottom: 1px solid #ccc;
	border-top: 1px solid #ccc;
}
	
#nav li {
	float: left; }
#nav li a {
	display: block;
	padding: 8px 15px;
	text-decoration: none;
	font-weight: bold;
	color: #069;
	border-right: 1px solid #ccc;}
/* ----------- My Form ----------- */
.myform{
margin:0 auto;
width:900px;
padding:14px;
}

/* ----------- formish ----------- */
#formish{
border:solid 2px #b7ddf2;
background:#ebf4fb;
}
#formish h1 {
font-size:14px;
font-weight:bold;
margin-bottom:8px;
}
#formish p{
font-size:11px;
color:#666666;
margin-bottom:20px;
border-bottom:solid 1px #b7ddf2;
padding-bottom:10px;
}
#formish label{
display:block;
font-weight:bold;
text-align:right;
width:140px;
float:left;
}
#formish input{
font-size:12px;
padding:4px 2px;
border:solid 1px #aacfe4;
width:200px;
margin:2px 0 20px 10px;
}
#formish textarea{
font-size:12px;
padding:4px 2px;
border:solid 1px #aacfe4;
width:600px;
margin:2px 0 20px 140px;
}
#formish select{
font-size:12px;
padding:4px 2px;
border:solid 1px #aacfe4;
width:80px;
margin:2px 0 20px 10px;
}

#formish button{
clear:both;
margin-left:150px;
width:125px;
height:31px;
background:#666666 url(img/button.png) no-repeat;
text-align:center;
line-height:31px;
color:#FFFFFF;
font-size:11px;
font-weight:bold;
}

#tvdetails{
margin-left:10px;
line-height:12px;
font-size:11px;
font-weight:normal;
}
#tvdetails .title{
display:block;
font-weight: bold;
text-align:right;
float:left;
width: 125px;
}
#tvdetails .contains{
margin:2px 0 20px 10px;
padding:4px 2px;
}
#tvdetails textarea{
border:0px;
font-family: Verdana;
width:400px;
height: 80px;
border: 1px solid #cccccc;
padding: 5px;
overflow:auto;
}
#tvdetails select{
border:0px;
font-family: Verdana;
width: 150px;
}
#results {
border:1px solid #aacfe4;
border-bottom: 0px;
font-size: 10px;
font-family: arial;
padding: 0px;
display: none;
}
#results div{
border-bottom: 1px solid #aacfe4;
padding: 3px;
}
#results .selected{
background-color: #666;
color: #fff;
}
#results .unselected{
background-color: #fff;
color: #666;
}

